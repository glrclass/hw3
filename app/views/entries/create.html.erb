<%= form_with :model: @entry do |form| %>
  <p>
    <%= form.label "title" %>
    <%= form.text_field "title" %>
  </p>
  <p>
    <%= form.label "occurred_on" %> <!-- might need to be date?-->
    <%= form.text_field "occurred_on" %>
  </p>
  <p>
    <%= form.label "description" %>
    <%= form.text_field "description" %>
  </p>
  <p>
    <%= form.label "place_id" %>
    <%= form.text_field "place_id" %>
  </p>
  <p>
    <%= form.submit %>
  </p>

<% end %>